		"operators": {
			"patterns": [
				{
					"match": "\\s*(?:<|<=|=|==|>=|>|~=)\\s*",
					"name": "keyword.operator.relation.oml"
				},
				{
					"match": "\\s*(?:\\+|-|\\*|/|\\\\|\\^)\\s*",
					"name": "keyword.operator.arithmetic.oml"
				},
				{
					"match": "\\s*(?:\\|\\||&&)\\s*",
					"name": "keyword.operator.boolean.oml"
				},
				{
					"match": "(?:\\.\\.\\.|\\=|:|\\~|@)",
					"name": "keyword.other.oml"
				}
			]
		},
		"number": {
			"patterns": [
				{
				 "comment": "hexa, octal, decimal, double, scientific and complex numbers",
				 "match": "((\\b0x[0-9a-fA-F]+\\b)|(\\b0o[0-7]+\\b)|((\\b[0-9]+\\.?[0-9]*)|(\\.[0-9]+))([eE][+-]?[0-9]+)?([ij])?)",
				 "name": "constant.numeric.oml"
				}
			]
		},
		"transpose": {
            "match": "(\\]|\\))((?:\\.)?'*)",
            "name": "keyword.operator.transpose.oml"
        },
		"strings": {
			"patterns": [{
				"begin": "'",
				"beginCaptures": {
					"0": {
					"name": "punctuation.definition.string.begin.oml"
					}
				},
				"end": "'(?!')",
				"endCaptures": {
					"0": {
					"name": "punctuation.definition.string.end.oml"
					}
				},
				"name": "string.quoted.single.oml",
				"patterns": [{
					"match": "\\\\.",
					"name": "constant.character.escape.oml"
				}]
			}]
		},
		"comment": {
			"patterns": [
			  {
				"include": "#comment_block"
			  },
			  {
				"begin": "%",
				"beginCaptures": {
				  "0": {
					"name": "punctuation.definition.comment.oml"
				  }
				},
				"end": "\\n",
				"name": "comment.line.percentage.oml"
			  }
			]
		  },
		  "comment_block": {
			"patterns": [
			  {
				"begin": "%{",
				"beginCaptures": {
				  "0": {
					"name": "punctuation.definition.comment.begin.oml"
				  }
				},
				"end": "%}",
				"endCaptures": {
				  "0": {
					"name": "punctuation.definition.comment.end.oml"
				  }
				},
				"name": "comment.block.percentage.oml",
				"patterns": [
				  {
					"include": "#comment_block"
				  }
				]
			  }
			]
		}
