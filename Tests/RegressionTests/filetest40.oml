fname = 'FileManipulation/FileToRead.txt'
fid = fopen(fname)
fgets(fid)
ftell_check = ftell(fid);
if (ispc && ftell_check == 11) || (isunix && ftell_check == 10)
	disp('ftell returns expected value: 11 for Windows, 10 for Linux')
else
	printf('ftell unexpected return value: %d\n', ftell_check)
end
fclose(fid)
