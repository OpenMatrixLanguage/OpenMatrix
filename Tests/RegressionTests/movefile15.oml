% movefile-note movefile files with spaces using wildcard 

returndir = pwd;
cd FileManipulation

cd movefile

disp('move file with spaces')
command = 'movefile(''test1.txt'', ''test 1.txt'')'
eval(command)

dir

disp('move second file with spaces back')
command = 'r1 = movefile(''test2.txt'', ''test 2.txt'')'
eval(command)

dir

disp('move wildcard files with spaces to dir1')
command = 'r1 = movefile(''test*.txt'', ''dir1'')'
eval(command)

dir

command = 'dir dir1'
eval(command)

command = 'dir dir2'
eval(command)

disp('move wildcard files with spaces from dir1 to dir 2')
command = 'r1 = movefile(''dir1/test*.txt'', ''dir2'')'
eval(command)

dir

command = 'dir dir1'
eval(command)

command = 'dir dir2'
eval(command)

disp('move wildcard files with spaces from dir2 to current dir')
command = 'r1 = movefile(''dir2/test*.txt'', ''.'')'
eval(command)

dir

command = 'dir dir1'
eval(command)

command = 'dir dir2'
eval(command)

disp('move file with spaces to remove spaces')
command = 'movefile(''test 1.txt'', ''test1.txt'')'
eval(command)

dir

