addpath([getenv('OML_ROOT') '/Tests/RegressionTests/functions']);
addToolbox omlstatistics

rand('seed', 2023);
v = randperm(100);
A = reshape(v, 10, 10);
A = A' * A;
[v,d] = eig(A);
d(2,2) = -d(2,2);
d(3,3) = -d(3,3);
d(5,5) = -d(5,5);
d(7,7) = -d(7,7);
d(8,8) = -d(8,8);
A = v*d*v';
[L,D,P] = ldl(A, 'lower');
C = A - P*L*D*L'*P';
L = lowbitcleanup(L, 7)
D = lowbitcleanup(D, 7)
P = lowbitcleanup(P, 7)
C = lowbitcleanup(C, 21)
