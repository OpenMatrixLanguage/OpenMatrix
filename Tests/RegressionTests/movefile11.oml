returndir = pwd;
cd FileManipulation

cd movefile
dir

disp('copy test files to dir2 prior to movefile test to cause fail')
command = 'copyfile(''test*'', ''dir2'')'
eval(command)

command = 'dir dir1'
eval(command)

command = 'dir dir2'
eval(command)

