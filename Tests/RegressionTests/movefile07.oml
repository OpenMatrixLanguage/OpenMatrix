returndir = pwd;
cd FileManipulation

cd movefile
dir

command = 'movefile(''dir1'', ''dir2'')'
eval(command)

dir

command = 'dir dir1'
eval(command)

command = 'dir dir2'
eval(command)

command = 'movefile(''dir2/dir1'', ''dir1'')'
