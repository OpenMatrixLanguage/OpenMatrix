addtoolbox omlDiffEq

function dy = func(t,y,pie,four)
   b = pie/four;
   dy = zeros(3,1);
   dy(1) = -b*y(2);
   dy(2) = y(1);
   dy(3) = y(1)*y(2);
end

options = odeset('RelTol', 1.0e-4, 'AbsTol', [1.0e-5, 1.0e-6, 1.0e-7]);

% interval mode test
handle = @(t,y) func(t,y,pi,4);
[t,y] = ode45(handle, [0:1:12], [0,1,1], options)
