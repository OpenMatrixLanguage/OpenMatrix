addtoolbox omlstatistics

a = 2;
b = -1;
c = 1.5;
x = [1:1:25];
y = [1:1:25];
z = [1:1:25];

i = 0;
while (i<5)
	j = 1;
	while (j<=5)
		x(i*5+j) = i+1;
		y(i*5+j) = j+1;
		z(i*5+j) = a + b*log(0.5*(i+1)) + c*(j+1);
		j = j+1;
	end
	i = i+1;
end

rv = [0.09 0.07 -0.02 -0.15 0.02 -0.11 0.04 -0.10 0.05 -0.13 0.15 -0.15 0.19 -0.05 -0.16 0.13 -0.01 0.09 0.03 -0.06 0.13 0.12 0.05 -0.10 -0.12];
z = z + 10*rv;

A = [ones(25,1), log(0.5*x)', y'];
B = z';

[b,bi,r,ri,stats] = regress(B, A)

