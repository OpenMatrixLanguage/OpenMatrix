addtoolbox omlSignals

% define and plot test signal
n = 576;			% number of points (9*64)
fs = 512;			% sampling frequency
ts = 1/fs;			% sampling time
t = [0:1:(n-1)]*ts;
f1 = 20;			% signal component frequency
f2 = 48;			% signal component frequency
omega1 = 2*pi*f1;
omega2 = 2*pi*f2;
signal = 6 + 10*cos(omega1*t+pi/6) + 8*cos(omega2*t+pi/3);

%pwelch(signal, window, overlap, fftSize, sampFreq, sideOpt)
overlap = 64;
mask = 1000000;

window = hamming(128, 'periodic');
[Pxx,f] = pwelch(signal, [], overlap, 128, fs, 'twosided');
round(mask*Pxx([1,2,3,5,6,7,8,12,13,14])')/mask
