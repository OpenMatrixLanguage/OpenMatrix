addtoolbox omlSignals

function result = EllipOrdHP()
	result = 'fail';
	Wp = 400;
	Ws = 300;
	Rp = 1;
	Rs = 20;
	[order, Wn] = ellipord(Wp, Ws, Rp, Rs, 's')

	[num, den] = ellip(order, Rp, Rs, Wp, 'high', 's');
	w = [0,Ws,Wp];
	m = abs(freqs(num,den,w));
	dB = -20 * log10(m);

	if (mod(order,2) == 0)
		if (abs(dB(1) - Rp) > 1.0e-4)
			break;
		end
	else
		if (m(1) > 1.0e-4)
			return;
		end
	end

	if (dB(2) < Rs)
		return;
	end

	if (abs(dB(3) - Rp) > 1.0e-4)
		return;
	end

	[num, den] = ellip(order, Rp, Rs, Wn, 'high', 's');
	w = [0,Ws,Wp];
	m = abs(freqs(num,den,w));
	dB = -20 * log10(m);

	if (mod(order,2) == 0)
		if (abs(dB(1) - Rp) > 1.0e-4)
			break;
		end
	else
		if (m(1) > 1.0e-4)
			return;
		end
	end

	if (abs(dB(2) - Rs) > 1.0e-4)
		return;
	end

	if (dB(3) > Rp)
		return;
	end

	result = 'pass';
end

ellipordHP = EllipOrdHP()
