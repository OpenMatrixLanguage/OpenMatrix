addtoolbox omlSignals

function result = Cheb2OrdBS()
	result = 'fail';
	Wp = [300, 780];
	Ws = [400, 600];
	Rp = 1;
	Rs = 20;
	[order, Wn] = cheb2ord(Wp, Ws, Rp, Rs, 's')

	[num, den] = cheby2(order, Rs, Ws, 'stop', 's');
	wcc = sqrt(Ws(1)*Ws(2));
	w = [0,Wp(1),Ws(1),wcc,Ws(2),Wp(2)];
	m = abs(freqs(num,den,w));
	dB = -20 * log10(m);

	if (abs(m(1) - 1.0) > 1.0e-4)
		break;
	end

	if (dB(2) > Rp)
		return;
	end

	if (abs(dB(3) - Rs) > 1.0e-4)
		return;
	end

	if (mod(order,2) == 0)
		if (abs(dB(4) - Rs) > 1.0e-4)
			break;
		end
	else
		if (m(4) > 1.0e-4)
			break;
		end
	end

	if (abs(dB(5) - Rs) > 1.0e-4)
		return;
	end

	if (dB(6) > Rp)
		return;
	end

	[num, den] = cheby2(order, Rs, Wn, 'stop', 's');
	wcc = sqrt(Wn(1)*Wn(2));
	w = [0,Wp(1),Ws(1),wcc,Ws(2),Wp(2)];
	m = abs(freqs(num,den,w));
	dB = -20 * log10(m);

	if (abs(m(1) - 1.0) > 1.0e-4)
		break;
	end

	if (dB(2) > Rp)
		return;
	end

	if (dB(3) < Rs)
		return;
	end

	if (mod(order,2) == 0)
		if (abs(dB(4) - Rs) > 1.0e-4)
			break;
		end
	else
		if (m(4) > 1.0e-4)
			break;
		end
	end

	if (dB(5) < Rs)
		return;
	end

	if (abs(dB(6) - Rp) > 1.0e-4)
		return;
	end

	result = 'pass';
end

cheb2ordBS = Cheb2OrdBS()
