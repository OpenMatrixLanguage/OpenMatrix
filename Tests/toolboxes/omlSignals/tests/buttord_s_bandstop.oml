addtoolbox omlSignals

function result = ButtOrdBS()
	result = 'fail';
	Wp = [300, 770];
	Ws = [400, 600];
	Rp = 20 * log10(sqrt(2));
	Rs = 20;
	[order, Wn] = buttord(Wp, Ws, Rp, Rs, 's')

	[num, den] = butter(order, Wp, 'stop', 's');
	wcc = sqrt(Wp(1)*Wp(2));
	w = [0,Wp(1),Ws(1),wcc,Ws(2),Wp(2)];
	m = abs(freqs(num,den,w));
	dB = -20 * log10(m);

	if (abs(m(1) - 1.0) > 1.0e-4)
		return;
	end

	if (abs(dB(2) - Rp) > 1.0e-4)
		return;
	end

	if (dB(3) < Rs)
		return;
	end

	if (m(4) > 1.0e-4)
		return;
	end

	if (dB(5) < Rs)
		return;
	end

	if (abs(dB(6) - Rp) > 1.0e-4)
		return;
	end

	[num, den] = butter(order, Wn, 'stop', 's');
	wcc = sqrt(Wn(1)*Wn(2));
	w = [0,Wp(1),Ws(1),wcc,Ws(2),Wp(2)];
	m = abs(freqs(num,den,w));
	dB = -20 * log10(m);

	if (abs(m(1) - 1.0) > 1.0e-4)
		return;
	end

	if (dB(2) > Rp)
		return;
	end

	if (dB(3) < Rs)
		return;
	end

	if (m(4) > 1.0e-4)
		return;
	end

	if (abs(dB(5) - Rs) > 1.0e-4)
		return;
	end

	if (dB(6) > Rp)
		return;
	end

	result = 'pass';
end

buttordBS = ButtOrdBS()
