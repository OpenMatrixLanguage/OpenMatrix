addpath([getenv('OML_ROOT') '/Tests/RegressionTests/functions']);
addtoolbox omlsignals

n = 1025;
fs = 1024;
ts = 1 / fs;
t = [0:ts:(n-1)*ts];
y = sin(19.7*t) + 0.5*cos(38.3*t) + 0.1*sin(63.5*t+1/6) + 0.1*sin(96.1*t+1/3);
data = abs(y); % positive values

[pks tp extra] = findpeaks(data, fs, 'MinPeakHeight', 0.9, 'MinPeakWidth', 30 * ts);
extra.parabol.pp = lowbitcleanup(extra.parabol.pp, 20);
pks
tp
extra
%plot(t,data);
%hold;
%scatter(tp,pks);